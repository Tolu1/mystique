{
	"name": "mystique",
	"displayName": "Mystique",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mystique.whenActive"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mystique.whenActive",
				"title": " Start Mystique",
				"category": "Mystique"
			},
			{
				"command": "mystique.shapeShift",
				"title": "Run with Mystique",
				"category": "Mystique"
			},
			{
				"command": "mystique.terminate",
				"title": "Stop Mystique",
				"category": "Mystique"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "mystique.shapeShift",
					"group": "Grp@Mystique"
				},
				{
					"command": "mystique.terminate",
					"group": "Grp@Mystique"
				}
			],
			"explorer/context": [
				{
					"command": "mystique.shapeShift",
					"group": "navigation@Mystique"
				},
				{
					"command": "mystique.terminate",
					"group": "navigation@Mystique"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/uuid": "^8.3.1",
		"acorn": "^8.5.0",
		"recast": "^0.20.5",
		"fs-extra": "^10.0.0",
		"uuid": "^8.3.2"
	}
}
